# k6 Distributed Test Configuration
# Copy this file to .env and modify as needed

# Number of worker replicas (default: 2)
WORKER_REPLICAS=2

# Test script to run (relative to scripts/ directory)
TEST_SCRIPT=test.js

# Master API port
MASTER_PORT=6565

# Log level (debug, info, warn, error)
LOG_LEVEL=info

# Test duration and target users (modify in test script)
# These are just examples - actual values are in the test script
EXAMPLE_DURATION=2m
EXAMPLE_TARGET_USERS=10
